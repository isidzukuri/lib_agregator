<script>
  window.tags_list = <%== @all_items.to_json %>;
</script>

<div class="input-group">
  <input id="tags_autocomplete" class="form-control" placeholder="Пошук тегів:">
</div>

<div id="tags_results"></div>
<hr/>

<div class="tags_pool">
<% @items.each do |item|%>
  <a href='/tags/<%= item.seo%>' class="label label-default"><%= item.uk%></a>
<% end %>
</div>
<%= render partial: "bibliotheca/shared/paginator" %>

<%= render partial: "bibliotheca/shared/book_search_form" %> 

<% content_for(:title, 'Книги по тегах') %>
